{
  "name": "express-cjs",
  "version": "0.0.1",
  "description": "coffee-script / jade / stylus(with kouto-swiss) middlewares for Single Page Application using Express4",

  "bin": {
    "cjs": "cjs"
  },
  "files": [
    "cjs",
    "index.js",
    "lib"
  ],

  "scripts": {
    "start": "jasminetea --watch",
    "test": "jasminetea --lint --cover --report",

    "prepublish": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "coffee --bare --compile index.coffee",
    "postcompile": "coffee --output lib --bare --compile src"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  },

  "dependencies": {
    "browserify-middleware": "^6.0.0",
    "browserify-plain-jade": "^0.2.2",
    "coffee-script": "^1.10.0",
    "coffeeify": "^1.1.0",
    "express": "^4.13.1",
    "jade": "^1.11.0",
    "kouto-swiss": "^0.11.13",
    "minimist": "^1.2.0",
    "stylus": "^0.51.1"
  },
  "devDependencies": {
    "ghooks": "^0.3.2",
    "jasminetea": "^0.2.1",
    "object-parser-cli": "0.0.1-alpha.1",
    "supertest": "^1.1.0"
  },
  
  "keywords": [
    "express",
    "spa"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/express-cjs.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/express-cjs/issues"
  },
  "homepage": "https://github.com/59naga/express-cjs#readme"
}
